#include <stdio.h>
#include <stdlib.h>

int main()
{
    double A[2][3] = {15,52,29,47,34,85};
    double B[2][3] = {5,13,11,8,7,-10};
    double C[2][3];
    int i, j;


    for(i = 0; i < 2; i++)
        for(j = 0; j < 3; j++)
            C[i][j] = A[i][j] + B[i][j];


            printf("Resultado matriz terceira em %.3f[i][j]\n", C[2][3]);







    return 0;
}








#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("Num linhas matriz");
    int A; scanf("%d", &A);

    printf("Num linhas matriz");
    int B; scanf("%d", &B);

    int matriz[A][B];
    int vetor[B];
    int resultado[A];

    scanf("%d", &matriz[A][B]);
    scanf("%d", &vetor[B]);

    matriz[A][B]*vetor[B];

    for(int i = 0; i < matriz[A]; i++)
        for(int i = 0; i < vetor[B]; i++)
        [i]+= matriz[i][j]*vetor[j];

        printf("%d[i][j] %d[j]", resultado[A][B]);








    return 0;
}
